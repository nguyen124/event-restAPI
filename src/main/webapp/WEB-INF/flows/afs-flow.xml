<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow 
        http://www.springframework.org/schema/webflow/spring-webflow-2.4.xsd">

	<action-state id="begin">
		<evaluate
			expression="requestParameters.role=='Administrator'?'yes':'no'"></evaluate>
		<transition on="yes" to="index">
			<set name="flowScope.user" value="requestParameters.user" />
		</transition>
		<transition on="no" to="new"></transition>
	</action-state>

	<view-state id="index" view="/WEB-INF/index.jsp" />

	<view-state id="new" view="/WEB-INF/jsp/newAccount.jsp">

	</view-state>
</flow>